CREATE TABLE `Evidence` (
	id INTEGER NOT NULL, 
	_name VARCHAR(100), 
	doc_pointer VARCHAR(100), 
	quote VARCHAR(4000), 
	reviewer VARCHAR(100), 
	timestamp VARCHAR(100), 
	evidence_type VARCHAR(100), 
	evidence_class VARCHAR(100), 
	value FLOAT, 
	object_dose FLOAT, 
	precip_dose FLOAT, 
	numb_subjects INTEGER, 
	enzyme_system VARCHAR(100), 
	dose FLOAT, 
	assump_list_id INTEGER, 
	PRIMARY KEY (id)
);
