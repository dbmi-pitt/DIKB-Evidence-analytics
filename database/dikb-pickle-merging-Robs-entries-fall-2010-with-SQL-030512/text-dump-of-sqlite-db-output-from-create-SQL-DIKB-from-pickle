CREATE TABLE `Assertions` (
	id INTEGER NOT NULL, 
	_name VARCHAR(100), 
	object VARCHAR(100), 
	slot VARCHAR(100), 
	value VARCHAR(100), 
	ready_for_classification VARCHAR(100), 
	assert_by_default VARCHAR(100), 
	evidence_rating VARCHAR(100), 
	cont_val VARCHAR(100), 
	numeric_val VARCHAR(100), 
	assert_class VARCHAR(100), 
	evidence_for INTEGER, 
	evidence_against INTEGER, 
	PRIMARY KEY (id)
