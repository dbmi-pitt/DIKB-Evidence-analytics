CREATE TABLE `ESlot` (
	id INTEGER NOT NULL, 
	range_id INTEGER, 
	type VARCHAR(100), 
	d_slot_id INTEGER, 
	value_string VARCHAR(100), 
	value_numeric FLOAT, 
	value_boolean BOOLEAN, 
	PRIMARY KEY (id)
);
