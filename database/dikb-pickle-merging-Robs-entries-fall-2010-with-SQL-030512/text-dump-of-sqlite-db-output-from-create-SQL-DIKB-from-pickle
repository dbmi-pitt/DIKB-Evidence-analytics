CREATE TABLE `Metabolite` (
	id INTEGER NOT NULL, 
	_name VARCHAR(100), 
	metabolite BOOLEAN, 
	primary_total_clearance_mechanism INTEGER, 
	primary_metabolic_clearance_enzyme INTEGER, 
	primary_total_clearance_enzyme INTEGER, 
	in_vitro_probe_substrate_of_enzyme INTEGER, 
	`in_viVo_probe_substrate_of_enzyme` INTEGER, 
	in_vitro_selective_inhibitor_of_enzyme INTEGER, 
	`in_viVo_selective_inhibitor_of_enzyme` INTEGER, 
	pceut_entity_of_concern INTEGER, 
	maximum_concentration INTEGER, 
	minimum_therapeutic_dose INTEGER, 
	maximum_therapeutic_dose INTEGER, 
	assumed_effective_dose INTEGER, 
	substrate_of INTEGER, 
	is_not_substrate_of INTEGER, 
	inhibits INTEGER, 
	does_not_inhibit INTEGER, 
	has_metabolite INTEGER, 
	permanently_deactivates_catalytic_function INTEGER, 
	does_not_permanently_deactivate_catalytic_function INTEGER, 
	inhibition_constant INTEGER, 
	induces INTEGER, 
	increases_auc INTEGER, 
	`sole_PK_effect_alter_metabolic_clearance` INTEGER, 
	PRIMARY KEY (id)
);
