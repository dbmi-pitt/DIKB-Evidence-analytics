CREATE TABLE `Value` (
	id INTEGER NOT NULL, 
	value_id INTEGER, 
	value_string VARCHAR(100), 
	value_numeric INTEGER, 
	value_boolean BOOLEAN, 
	PRIMARY KEY (id)
);
