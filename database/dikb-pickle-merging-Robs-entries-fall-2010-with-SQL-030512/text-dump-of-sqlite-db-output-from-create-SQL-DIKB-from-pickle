CREATE TABLE `EMultiSlot` (
	id INTEGER NOT NULL, 
	range_id INTEGER, 
	type VARCHAR(100), 
	value_id INTEGER, 
	d_slot_id INTEGER, 
	PRIMARY KEY (id)
);
